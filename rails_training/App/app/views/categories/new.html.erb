<h1>Categories#new</h1>

<%= form_with scope: :category,url: categories_path, local: true do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
<p>
    <%= f.label :Parent_id %> <br>

<%= f.collection_select :parent_id, Category.where(parent_id: nil) , :id, :name , include_blank: true %>
</p>
 <p>
    <%= f.submit %>
  </p>
<% end %>


